{
  "name": "vue-naive-admin",
  "type": "module",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode dev",
    "mock": "vite --mode mock",
    "prod": "vite --mode prod",
    "build:dev": "vite build --mode dev",
    "build:prod": "vite build --mode prod",
    "preview:dev": "vite preview --mode dev",
    "preview:prod": "vite preview --mode prod",
    "deploy:dev": "pnpm build:dev && cross-env NODE_ENV=dev node ./deploy/index.js",
