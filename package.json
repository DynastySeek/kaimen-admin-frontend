    "cross-env": "^10.1.0",
    "eslint": "^9.29.0",
    "eslint-plugin-format": "^1.0.1",
    "esno": "^4.8.0",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.3",
    "less": "^4.4.1",
    "lint-staged": "^16.1.2",
    "mockjs": "^1.1.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "simple-git-hooks": "^2.13.0",
