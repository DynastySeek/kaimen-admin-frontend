    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.544.0",
    "naive-ui": "^2.42.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.3.0",
    "ua-parser-js": "^2.0.5",
    "vue": "^3.5.17",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.5.1",
    "vue3-video-play": "^1.3.2",
    "xlsx": "^0.18.5"
